    "verror": "^1.10.1",
    "yieldable-json": "^2.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/hostile": "^1.3.0",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.0.0",
    "@types/verror": "^1.10.5",
    "@types/yieldable-json": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
