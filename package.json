    "eslint-plugin-prettier": "^4.2.1",
    "javascript-obfuscator": "^4.0.0",
    "pkg": "^5.8.0",
    "prettier": "^2.8.1",
    "resedit": "^1.6.0",
    "typescript": "^4.8.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-obfuscator": "^3.5.1"
